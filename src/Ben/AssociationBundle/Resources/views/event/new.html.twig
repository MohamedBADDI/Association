{% extends "BenAssociationBundle::layout.html.twig" %}
{% block title %}
Tableau de bord | {{ parent() }}
{% endblock %}
{% block body %}
<h2><span class="fui-time"></span> event creation</h2>
<form id="createForm" class="form-horizontal" action="{{ path('event_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    
    <div class="form-group">
      <div class="col-md-offset-4 col-md-4">
        <button type="submit" class="btn btn-primary btn-embossed"><span class="glyphicon glyphicon-edit"></span> Modifier</button>
      </div>
    </div>
</form> 


{% endblock body %}